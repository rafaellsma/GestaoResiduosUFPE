<h1>Lista de usuários</h1>

<div class="well">
  <div class="tab-content">
    <div class="tab-pane active in" id="home">
      <div class="row">
        <table class="table table-striped custab">
        <thead>
            <tr>
                <th>Nome</th>
                <th>Email</th>
                <th>Laboratórios</th>
                <th>Ramal</th>
            </tr>
        </thead>
          <%= render partial: 'user', locals: {users: @users} %>
        </table>
        </div>
    </div>
    </div>
</div>
<% @users.each do |user| %>
  <%= render partial: 'modal_user', locals: {user: user} %>
<% end %>
